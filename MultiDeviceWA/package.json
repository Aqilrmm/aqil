{
  "name": "whatsapp-multidevice-api",
  "version": "1.0.0",
  "description": "WhatsApp Multi-Device API using whatsapp-web.js with Socket.io support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "clean": "rm -rf sessions/* qr-codes/* logs/*",
